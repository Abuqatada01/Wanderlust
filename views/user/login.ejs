<% layout("./layouts/boilerplate") %>

    <div class="row">
        <div class="col-8 offset-2 mt-3">
            <h4 class="col-6 offset-3">Login Wanderlust</h4>
            <form method="post" action="/login" class="needs-validation" novalidate>
                <div class="mb-3"> <label for="username" class="form-label">Username </label>
                  <input name="username" placeholder="Enter username" id="username" type="text" class="form-control" required  />
                  <div class="valid-feedback">
                    Looks good!
                  </div>
                </div>
               
                <div class="mb-3"> <label for="password" class="form-label">Password </label>
                  <input name="password" placeholder="Enter password" type="password" id="password" class="form-control" required  />
                  <div class="valid-feedback">
                    Looks good!
                  </div>
                </div>
               
      
                <button class="btn btn-dark">Login</button>
              </form>
        </div>
    </div>